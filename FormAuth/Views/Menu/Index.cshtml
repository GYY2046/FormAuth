@model IList<FormAuth.Models.Permission>
@{
    ViewBag.Title = "Index";
}

<div>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>ControllerId</th>
                <th>ControllerName</th>
                <th>ControllerCnName</th>
                <th>ActionId</th>
                <th>ActionName</th>
                <th>ActionCnName</th>
            </tr>            
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.CId</td>
                    <td>@item.CName</td>
                    <td>@item.ConCnName</td>
                    <td>@item.AId</td>
                    <td>@item.AName</td>
                    <td>@item.ActCnName</td>
                </tr>
            }
        </tbody>
    </table>
</div>